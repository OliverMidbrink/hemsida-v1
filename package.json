{
  "name": "hemsida",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "client": "vite",
    "server": "node server/index.js",
    "python-server": "cd python-api && python3 -m uvicorn main:app --host localhost --port 8000 --reload",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "setup": "node scripts/setup-dev.js",
    "dev:all": "concurrently \"npm run server\" \"npm run python-server\" \"npm run client\"",
    "dev:js": "concurrently \"npm run server\" \"npm run client\"",
    "dev:host": "concurrently \"npm run server\" \"vite --host\"",
    "dev:auth": "node scripts/dev-with-auth.js",
    "start": "node server/index.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.16.14",
    "better-sqlite3": "^9.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "uuid": "^9.0.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "vite": "^5.0.8"
  }
}
